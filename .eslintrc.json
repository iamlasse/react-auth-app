{
	"extends": [
		"airbnb",
		"plugin:jsx-a11y/recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 2015
	},

	"parser": "babel-eslint",

	"plugins": [
		"jsx-a11y"
	],
	"rules": {
		"comma-dangle": 0,
		"no-tabs": 0,
		"no-mixed-spaces-and-tabs": "off",
		"react/jsx-indent": 0,
		"react/jsx-indent-props": 0,
		"object-curly-newline": "off",
		"indent": "off"
	}
}
