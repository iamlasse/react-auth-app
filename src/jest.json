{
	"verbose": true,
	"setupTestFrameworkScriptFile": "<rootDir>src/setupTests.js",
	"showCoverageOnLoad": true,
	"collectCoverageFrom": [
		"src/**/*.{js,jsx}",
		"!src/**/store/**/logic",
		"!<rootDir>/node_modules/",
		"!<rootDir>/path/to/dir/"
	],
	"coverageThreshold": {
		"global": {
			"branches": 80,
			"functions": 80,
			"lines": 80,
			"statements": -10
		}
	}
}
